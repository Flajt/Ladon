{
    "manifest_version": 2,
    "name": "Ladon",
    "version": "1.0",
    "background": {
      "scripts": [
        "browser-polyfill.js",
        "background.js"
      ]
    },
    "permissions":["activeTab","unlimitedStorage"],
    "icons": {
      "16": "img/16x16.ico",
      "48": "img/48x48.ico",
      "64": "img/64x64.ico"
    },    
    "browser_action": {
      "default_icon": "img/48x48.ico",
      "default_title": "Ladon",
      "default_popup": "/src/html/popup.html"
    },
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": [
        "browser-polyfill.js"
      ]
    }]
}